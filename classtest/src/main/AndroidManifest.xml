<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.classtest">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".animation.MainActivity" />
        <activity android:name=".animation.XMLAnimActivity" />
        <activity android:name=".animation.ViewAnimActivity" />
        <activity android:name=".animation.DrawableActivity" />
        <activity android:name=".datastorage.SharedPreActivity" />
        <activity android:name=".datastorage.InternalStorageActivity" />
        <activity android:name=".datastorage.CacheActivity" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".datastorage.ExternalStorageActivity" />
        <activity android:name=".animation.TransitionActivity" />
        <activity android:name=".datastorage.ContentProviderActivity" />
        <activity android:name=".datastorage.CPLoadActivity"></activity>
    </application>

</manifest>